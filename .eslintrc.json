{
    "parser": "@typescript-eslint/parser",
    "extends": "plugin:@typescript-eslint/recommended",
    "parserOptions": {
        "ecmaVersion": 10,
        "sourceType": "module"
    },
    "plugins": [
        "import",
        "angular"
    ],
    "rules": {
        "accessibility": "no-public"
    }
}
